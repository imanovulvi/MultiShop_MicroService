@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Admin/Views/Layout/Index.cshtml";
}


<div class="col-md-12">
    <div class="chart-box">
        <h4>Category Table</h4>
        <p>Add <code>.table-bordered</code> for borders on all sides of the table and cells.</p>
        <div class="bs-example" data-example-id="bordered-table">
            <form method="post" action="/admin/Brand/Create">
                <p>Name</p>
                <input type="text" name="Name" class="form-control" id="inputSuccess1" aria-describedby="helpBlock2">
                <br />
                <p>ImageUrl</p>
                <input type="text" name="ImageUrl" class="form-control" id="inputSuccess1" aria-describedby="helpBlock2">
                <br />
                <input type="submit" value="Create" class="btn btn-primary" />

            </form>

        </div>
    </div>
</div>

