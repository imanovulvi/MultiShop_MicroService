 @model ResultCompanyDTO
@{
    ViewData["Title"] = "Update";
    Layout = "~/Areas/Admin/Views/Layout/Index.cshtml";
}



<div class="col-md-12">
    <div class="chart-box">
        <h4>Category Table</h4>
        <p>Add <code>.table-bordered</code> for borders on all sides of the table and cells.</p>
        <div class="bs-example" data-example-id="bordered-table">
            <form method="post" action="/admin/CargoCompany/Update">
                <input type="hidden" name="Id" value="@Model.Id" />
                <br />
                <p>Name</p>
                <input type="text" name="Name" value="@Model.Name" class="form-control" id="inputSuccess1" aria-describedby="helpBlock2" />
                <br />
                

                <input type="submit" value="Update" class="btn btn-primary" />

            </form>

        </div>
    </div>
</div>


